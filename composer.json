{
    "name": "fusonic/rate-limit-bundle",
    "description": "Simple rate limiting based on routes.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Zangerle",
            "email": "michael.zangerle@fusonic.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Fusonic\\RateLimitBundle\\": "Fusonic/RateLimitBundle/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fusonic\\RateLimitBundle\\Tests\\": "Fusonic/RateLimitBundle/tests"
        }
    },
    "require": {
        "php": "^7.1.3",
        "symfony/http-kernel": "^3.4",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "symfony/dependency-injection": "^3.4",
        "symfony/config": "^3.4",
        "symfony/monolog-bundle": "^3.3",
        "symfony/framework-bundle": "^3.4",
        "symfony/monolog-bridge": "^3.4"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.4",
        "friendsofphp/php-cs-fixer": "^2.12",
        "phpstan/phpstan": "^0.9",
        "symfony/yaml": "^3.4"
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse Fusonic/RateLimitBundle/src --level=6 -c phpstan.neon"
    }
}
